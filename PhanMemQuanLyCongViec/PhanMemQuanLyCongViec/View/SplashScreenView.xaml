        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhanMemQuanLyCongViec.View"
        mc:Ignorable="d"
        Title="SplashScreenView" Height="450" Width="700">
    <Grid>
        <Image Width="{Binding Path = Width,ElementName=SplashScreenWindow}" Height="{Binding Path=Height,ElementName=SplashScreenWindow}"/>
    </Grid>
</Window>
